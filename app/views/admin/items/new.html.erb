<%= form_for [:admin, @item] do |f| %>
<%= f.label :title %><br />
<%= f.text_field :title  %><br />
<%= f.label :description %><br />
<%= f.text_field :description  %><br />
<%= f.label :price %><br />
<%= f.text_field :price %><br />
<%= f.label :image %><br />
<%= f.text_field :image %><br />

Categories: <%= f.collection_check_boxes(:category_ids, @categories, :id, :name) %><br /><br />
<%= f.submit %>
<% end %>
